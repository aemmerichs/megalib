module redux.demo.real-world.Dispatch

import redux.Dispatch where {
  [ dispatchUserRequestAction / ?dispatch,
    dispatchUserSuccessAction / ?dispatch,
    dispatchUserFailureAction / ?dispatch,
    dispatchRepoRequestAction / ?dispatch,
    dispatchRepoSuccessAction / ?dispatch,
    dispatchRepoFailureAction / ?dispatch,
    dispatchStarredRequestAction / ?dispatch,
    dispatchStarredSuccessAction / ?dispatch,
    dispatchStarredFailureAction / ?dispatch,
    dispatchStargazersRequestAction / ?dispatch,
    dispatchStargazersSuccessAction / ?dispatch,
    dispatchStargazersFailureAction / ?dispatch,
    dispatchResetErrorMessageAction / ?dispatch
  ]
}

